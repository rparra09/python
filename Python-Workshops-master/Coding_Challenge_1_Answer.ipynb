        "def quadraticEquation(a,b,c):\n",
        "  a = float(a)\n",
        "  b = float(b)\n",
        "  c = float(c)\n",
        "  \n",
        "  numerator1 = b*(-1) + (b**2 - 4*a*c)**(1/2)\n",
        "  numerator2 = b*(-1) - (b**2 - 4*a*c)**(1/2)\n",
        "  denumerator = 2*a\n",
        "\n",
        "  result1 = numerator1 / denumerator\n",
        "  result2 = numerator2 / denumerator\n",
        "  \n",
        "  return result1, result2"
      ],
